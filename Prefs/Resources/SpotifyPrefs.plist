<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Spotify -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Spotify</string>
			<key>isStaticText</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>io.c0ldra1n.mitsuhaxi-prefs</string>
			<key>label</key>
			<string>Enabled</string>
			<key>key</key>
			<string>MSHSpotifyEnabled</string>
			<key>alternateColors</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>io.c0ldra1n.mitsuhaxi-prefs</string>
			<key>label</key>
			<string>Enable FFT (experimental)</string>
			<key>key</key>
			<string>MSHSpotifyEnableFFT</string>
			<key>alternateColors</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>cellClass</key>
			<string>PFSimpleLiteColorCell</string>
			<key>libcolorpicker</key>
			<dict>
				<key>defaults</key>
				<string>io.c0ldra1n.mitsuhaxi-colors</string>
				<key>key</key>
				<string>MSHSpotifyWaveColor</string>
				<key>fallback</key>
				<string>#fcfcfc</string>
				<key>alpha</key>
				<true/>
			</dict>
			<key>label</key>
			<string>Wave color</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSStaticTextCell</string>
			<key>label</key>
			<string>Wave offset (top)</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>label</key>
			<string>Wave offset (top)</string>
			<key>defaults</key>
			<string>io.c0ldra1n.mitsuhaxi-prefs</string>
			<key>key</key>
			<string>MSHSpotifyWaveOffset</string>
			<key>min</key>
			<real>-150</real>
			<key>max</key>
			<real>150</real>
			<key>showValue</key>
			<true/>
			<key>isSegmented</key>
			<false/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSStaticTextCell</string>
			<key>label</key>
			<string>Frames per second</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>label</key>
			<string>Frames per second</string>
			<key>default</key>
			<real>10</real>
			<key>defaults</key>
			<string>io.c0ldra1n.mitsuhaxi-prefs</string>
			<key>key</key>
			<string>MSHSpotifyFps</string>
			<key>min</key>
			<real>5</real>
			<key>max</key>
			<real>240</real>
			<key>showValue</key>
			<true/>
			<key>isSegmented</key>
			<false/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>io.c0ldra1n.mitsuhaxi-prefs</string>
			<key>label</key>
			<string>Ignore ColorFlow</string>
			<key>key</key>
			<string>MSHSpotifyIgnoreColorFlow</string>
			<key>alternateColors</key>
			<true/>
		</dict>
		<!-- Credits -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>MitsuhaXI</string>
			<key>isStaticText</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Mitsuha, Mitsuha2 by c0ldra1n,
ported to iOS 11 by Ominousness.</string>
			<key>isStaticText</key>
			<true/>
		</dict>
	</array>
	<key>title</key>
	<string>Spotify - MitsuhaXI</string>
</dict>
</plist>